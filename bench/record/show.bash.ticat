help = show records of current bench
abbr = display

[arg]
host|h|H = ''
port|p|P = ''
db-name|db = ''
user = root
bench-begin|begin = ''

[arg2env]
bench.meta.host = host
bench.meta.port = port
bench.meta.db-name = db-name
bench.meta.user = user
bench.begin = bench-begin

[env]
bench.meta.host = may-read
bench.meta.port = may-read
bench.meta.db-name = may-read
bench.meta.user = may-read
bench.run.begin = may-read
bench.begin = may-read
